#results
	padding 90px 0 50px
.general-title-results
	margin-bottom: 0
.results-subtitle
	font-family: GM
	font-size: 30px
	font-weight: 600
	text-transform uppercase
	margin 15px 0
	text-align center
.results-subtitle-2
	display block
	padding 24px 20px
	width 100%
	max-width 600px
	margin 0 auto
	text-transform uppercase
	font-family: GM
	font-size: 30px
	font-weight: 300
	background-color: #cc922f
	text-align: center
	color #fff
.results-container
	border-radius: 40px
	background-color: #e6e6e6
	padding: 0 40px 60px
	position relative
	width 100%
	max-width 650px
	margin 50px auto 0
	&-img
		display block
		width 100%
		position relative
		left 0
		right 0
		margin 0 auto
		top -30px
	&-title
		font-family: GM
		font-size: 35px
		font-weight: 900
		text-transform uppercase
		color #19334d
		small
			font-size: 28px
			display block
			margin-bottom: 40px
.rect.rect-results
	width 270px
	height 300px
	background-color: #19334d
	right -50px
	bottom: -50px
	z-index 0
.results-img
	display block
	width 100%
	max-width 569px
	margin 150px auto 0
	position relative
	z-index 10